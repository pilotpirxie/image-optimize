{
  "name": "media-optimizer",
  "version": "1.0.0",
  "main": "./src/app.ts",
  "repository": "https://github.com/pilotpirxie/media-optimizer.git",
  "author": "pilotpirxie <10637666+pilotpirxie@users.noreply.github.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "heic-convert": "^1.2.4",
    "jimp": "^0.16.2"
